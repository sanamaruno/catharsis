 <h2>投稿一覧</h2>
      <table class="table">
        <thead>
          <tr>
            <th></th>
            <th>投稿日</th>
            <th>劇場</th>
            <th>タイトル</th>
            <th>尊い</th>
          </tr>
        </thead>
        <tbody>
        <% posts.each do |post| %>
          <tr>
            <td>
              <%= link_to user_path(post.user) do %>
                <%= attachment_image_tag post.user, :profile_image, :fill, 40, 40, fallback: "no_image.jpg", size:'40x40' %>
              <% end %>
            </td>
            <td>
              <%= post.created_at.strftime('%Y/%m/%d') %>
            </td>
            <td>
              <%= post.place %>
            </td>
            <td>
              <%= link_to post_path(post.id) do %>
                <%= post.title %>
              <% end %>
            </td>
            <td></td>
          </tr>
        <% end %>
        </tbody>
      </table>
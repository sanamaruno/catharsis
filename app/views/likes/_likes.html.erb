<% if post.liked_by?(current_user) %>
  <%= link_to post_likes_path(post), class: "btn btn-outline-warning", method: :delete, remote: true do %>
    <i class="fas fa-star"></i>
    <%= post.likes.count %>
  <% end %>
<% else %>
 <%= link_to post_likes_path(post), class: "btn btn-outline-warning", method: :post, remote: true do %>
    <i class="far fa-star"></i>
    <%= post.likes.count %>
  <% end %>
<% end %>